[
    {
        "settings": ["master"],
        "staticHandling":{
            "useRollups":false
        },
        "shaker":{
            "task": "raw",
            "minify": true,
            "routeBundle": {
                "/none": ["master", "primary", "secondary"]
            },
            "rollupConfig": {
                "//fetchViews": "none",
                "//fetchController": "none"
            }
        },
        "yui": {
           "//dependencyCalculations": "precomputed"
        },
        "specs": {
            "myhtmlframe": {
                "type": "ShakerHTMLFrameMojit",
                "config": {
                    "deploy": true,
                    "title": "Shaker Demo",
                    "child": {
                        "type": "master"
                    },
                    "assets": {
                        "top": {
                            "css": ["poc.css"]
                        }
                    }
                }
            }
        }
    },
    {
        "settings": [ "device:iphone" ],
        "selector": "iphone"
    },
    {
        "settings": [ "lang:es-ES" ],
        "selector": "esp"
    },
    {
        "settings": [ "environment:corp"],
        "shaker": {
            "task": "local"
        }
    },
    {
        "settings": [ "environment:test"],
        "shaker":{
            "module": "xxx-mobstor",
            "task": "mobstor",
            "taskConfig": {
                "mobstor": {
                    "ycs": "",
                    "host": "",
                    "cert": "",
                    "proxy": {
                        "host": "",
                        "port": 1111
                    }
                }
            },
            "routeBundle": {
                "index": ["primary", "secondary"]
            },
            "rollupConfig": {
                "//fetchViews": "none",
                "//fetchController": "none"
            }
        }
    }
    
]
